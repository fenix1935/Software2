<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profesor_ver_respuestas">

    <TableLayout
        android:layout_width="313dp"
        android:layout_height="399dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@drawable/tabla">

                <TextView
                    android:id="@+id/textView36"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tabla"
                    android:text="Grupo 1"
                    android:textAlignment="center"
                    android:textColor="#B36ADB"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/aa"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:text="-"
                    android:textSize="17sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@drawable/tabla">

                <TextView
                    android:id="@+id/textView34"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tabla"
                    android:text="Grupo 2"
                    android:textAlignment="center"
                    android:textColor="#B36ADB"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/contenido_g2"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:text="-"
                    android:textSize="17sp" />
            </LinearLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@drawable/tabla">

                <TextView
                    android:id="@+id/textView39"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tabla"
                    android:text="Grupo 3"
                    android:textAlignment="center"
                    android:textColor="#B36ADB"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/contenido_g3"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:text="-"
                    android:textSize="17sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@drawable/tabla">

                <TextView
                    android:id="@+id/textView38"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tabla"
                    android:text="Grupo 4"
                    android:textAlignment="center"
                    android:textColor="#B36ADB"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/contenido_g4"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:text="-"
                    android:textSize="17sp" />
            </LinearLayout>
        </TableRow>

    </TableLayout>

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar6"
        android:layout_width="match_parent"
        android:layout_height="63dp"
        android:background="@drawable/fondo_profesor"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cambio"
        android:textColor="@android:color/white"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar6"
        app:layout_constraintEnd_toEndOf="@+id/toolbar6"
        app:layout_constraintStart_toStartOf="@+id/toolbar6"
        app:layout_constraintTop_toTopOf="@+id/toolbar6" />
</androidx.constraintlayout.widget.ConstraintLayout>